t { HttpResponse, HttpRequest } from '../app/Request';\nimport { withRouter } from 'react-router';\n\ntype StaffDoctorsAndNurseProps=AppComponentProps&{\n    serialNumber:string\n}\n\n/**\n * 医护人员管理组件\n */\nclass StaffDoctorsAndNurse extends React.Component<StaffDoctorsAndNurseProps>{\n    state:{\n        /**编辑抽屉是否显示 */\n        drawerVisible:boolean;\n        /**当前列表数据 */\n        data?:Array<ApplyContentDoctorOrNurse>;\n        /**正在编辑的数据 */\n        editingData?:ApplyContentDoctorOrNurse;\n        /**正在编辑的数据在列表中的索引 */\n        currentIndex:number;\n    }= { \n        drawerVisible: false,\n        currentIndex:-1\n    };\n\n    /**\n     * 页面布局\n     */\n    render(){\n        if(!this.state.data){\n            return (<Row>\n                <Col span={24}>\n                    <Spin className=\"card-loading\" size=\"large\" tip=\"载入中...\" />\n                </Col>\n            </Row>);\n        }\n\n        const {getFieldDecorator} = this.props.form;\n        const {Meta}=Card;\n        const {Option}=Select;\n\n        const {data,editingData}=this.state;\n        const education=DictionaryLoader.instance.education();\n        const job=DictionaryLoader.instance.job();\n        const jobLabel=DictionaryLoader.instance.jobLabel;\n\n        return (\n            <div>\n                <Row>\n                    {data.map((person:ApplyContentDoctorOrNurse,index)=>(\n                        <Col span={5}>\n                            <Card\n                                actions={[\n                                    <Icon type=\"edit\" key=\"edit\" onClick={()=>this.showDrawer(index)} />,\n                                    <Icon type=\"delete\" key=\"delete\" />\n                                ]}\n                            >\n                                <Meta className=\"staff-meta\"\n                                    avatar={<img src={person.img}/>}\n                                    title={person.name}\n                                    description={\n                                        <p>\n                                            {jobLabel(person.position)}<br/>\n                                            {jobLabel(person.title)}<br/>\n                                            {person.dept}\n                                        </p>\n                                    }\n                                />\n                            </Card>\n                        </Col>\n                    ))}\n                    <Col span={5} offset={1}>\n                        <Button type=\"dashed\" \n                            style={{width:\"100%\",height:199}}\n                            onClick={()=>this.showDrawer(-1)}\n                        >\n                            <Icon type=\"plus\" /><br/>\n                            添加\n                        </Button>\n                    </Col>\n                </Row>\n                \n                {/* 医生和护士编辑抽屉 */}\n                {editingData?(\n                    <Drawer\n                        title={\n                            this.state.currentIndex===-1\n                                ?\"添加医护人员\"\n                                :`编辑人员（${editingData.name}）`\n                        } \n                        width={560}\n                        onClose={this.onClose}\n                        visible={this.state.drawerVisible}\n                    >\n                        <Form labelCol={{span:6}} wrapperCol={{span:18}}>\n                            <h4 className=\"title-form-sub\">信息填写</h4>\n                            <Row>\n                                <Col span={15}>\n                                    <Form.Item label=\"人员姓名\">{\n                                        getFieldDecorator(\"name\",{\n                                            initialValue:editingData.name,\n                                            rules:[\n                                                {required:true,message:\"人员姓名不能为空！\"}\n                                            ]\n                                        })(\n                                            <Input placeholder=\"请输入\" required={true}/>\n                                        )}\n                                    </Form.Item>\n                                </Col>\n                                <Col span={9}>\n                                    <Form.Item label=\"性别\" \n                                        labelCol={{span:8}} wrapperCol={{span:16}}\n                                    >{\n                                        getFieldDecorator(\"sex\",{\n                                            initialValue:(\"\"===editingData.sex?\"男\":editingData.sex),\n                                            rules:[\n                                                {required:false}\n                                            ]\n                                        })(\n                                            <Radio.Group \n                                            >\n                                                <Radio value=\"男\">男</Radio>\n                                                <Radio value=\"女\">女</Radio>\n                                            </Radio.Group>\n                                        )}\n                                    </Form.Item>\n                                </Col>\n                            </Row>\n                            <Row>\n                                <Col span={15}>\n                                    <Form.Item label=\"身份证号\">{\n                                        getFieldDecorator(\"idcard\",{\n                                            initialValue:editingData.idcard,\n                                            rules:[\n                                                {required:true,message:\"身份证号码不能为空！\"}\n                                            ]\n                                        })(\n                                            <Input placeholder=\"请输入\" \n                                            />\n                                        )}\n                                    </Form.Item>\n                                </Col>\n                                <Col span={9}>&nbsp;</Col>\n                            </Row>\n                            <Row>\n                                <Col span={15}>\n                                    <Form.Item label=\"毕业院校\">\n                                        <Input placeholder=\"请输入\" required={true}/>\n                                    </Form.Item>\n                                </Col>\n                                <Col span={9}>\n                                    <Form.Item label=\"学历\" \n                                        labelCol={{span:8}} wrapperCol={{span:16}}\n                                    >\n                                        <Select showSearch\n                                            placeholder=\"请选择\"\n                                            optionFilterProp=\"children\"\n                                            filterOption={(input, option:React.ReactElement) =>\n                                                option.props.children.toLowerCase().indexOf(input.toLowerCase()) >= 0\n                                            }\n                                        >\n                                            {education.map((data:DictionaryData,index:number)=>\n                                                <Option key={data.value}>{data.label}</Option>\n                                            )}\n                                        </Select>\n                                    </Form.Item>\n                                </Col>\n                            </Row>\n                            <Row>\n                                <Col span={15}>\n                                    <Form.Item label=\"职务职称\">{\n                                        getFieldDecorator(\"person.job\",{\n                                            rules:[\n                                                {required:false}\n                                            ]\n                                        })(<Cascader options={job} placeholder=\"请选择\" />)}\n                                    </Form.Item>\n                                </Col>\n                                <Col span={9}>\n                                    <Form.Item label=\"科室\"\n                                        labelCol={{span:8}} wrapperCol={{span:16}}\n                                    >\n                                        <Input placeholder=\"请输入\" required={true}/>\n                                    </Form.Item>\n                                </Col>\n                            </Row>\n                            <h4 className=\"title-form-sub\">上传内容</h4>\n                            <Row>\n                                <Col span={12}>\n                                    <Form.Item labelCol={{span:24}} wrapperCol={{span:24}}\n                                        label=\"身份证正面\" help=\"支持扩展名：.jpg .png\">{\n                                            getFieldDecorator(\"person.identity.front\",{\n                                                rules:[\n                                                    {required:true}\n                                                ]\n                                            })(\n                                                <Row>\n                                                    <Col span={10}>\n                                                        <Upload\n                                                            name=\"person.identity.front\"\n                                                            listType=\"picture-card\"\n                                                            className=\"avatar-uploader\"\n                                                            showUploadList={false}\n                                                        >\n                                                            <div>\n                                                                <Icon type=\"plus\" />\n                                                                <div className=\"ant-upload-text\">上传</div>\n                                                            </div>\n                                                        </Upload>\n                                                    </Col>\n                                                    <Col span={11} offset={1}>\n                                                        <Button type=\"dashed\" style={{height:102,width:102}}>\n                                                            <Icon type=\"file-image\" style={{fontSize:32}} />\n                                                            <br/>\n                                                            <span>示例图</span><Icon type=\"zoom-in\" />\n                                                        </Button>\n                                                    </Col>\n                                                </Row>\n                                            )}\n                                    </Form.Item>\n                                </Col>\n                                <Col span={12}>\n                                    <Form.Item labelCol={{span:24}} wrapperCol={{span:24}}\n                                        label=\"身份证背面\" help=\"支持扩展名：.jpg .png\">{\n                                            getFieldDecorator(\"person.identity.back\",{\n                                                rules:[\n                                                    {required:true}\n                                                ]\n                                            })(\n                                                <Row>\n                                                    <Col span={10}>\n                                                        <Upload\n                                                            name=\"person.identity.back\"\n                                                            listType=\"picture-card\"\n                                                            className=\"avatar-uploader\"\n                                                            showUploadList={false}\n                                                        >\n                                                            <div>\n                                                                <Icon type=\"plus\" />\n                                                                <div className=\"ant-upload-text\">上传</div>\n                                                            </div>\n                                                        </Upload>\n                                                    </Col>\n                                                    <Col span={11} offset={1}>\n                                                        <Button type=\"dashed\" style={{height:102,width:102}}>\n                                                            <Icon type=\"file-image\" style={{fontSize:32}} />\n                                                            <br/>\n                                                            <span>示例图</span><Icon type=\"zoom-in\" />\n                                                        </Button>\n                                                    </Col>\n                                                </Row>\n                                            )}\n                                    </Form.Item>\n                                </Col>\n                            </Row>\n                            <Row>\n                                <Col span={12}>\n                                    <Form.Item labelCol={{span:24}} wrapperCol={{span:24}}\n                                        label=\"二寸寸照\" help=\"支持扩展名：.jpg .png\">{\n                                            getFieldDecorator(\"person.photo\",{\n                                                rules:[\n                                                    {required:true}\n                                                ]\n                                            })(\n                                                <Row>\n                                                    <Col span={10}>\n                                                        <Upload\n                                                            name=\"person.photo\"\n                                                            listType=\"picture-card\"\n                                                            className=\"avatar-uploader\"\n                                                            showUploadList={false}\n                                                        >\n                                                            <div>\n                                                                <Icon type=\"plus\" />\n                                                                <div className=\"ant-upload-text\">上传</div>\n                                                            </div>\n                                                        </Upload>\n                                                    </Col>\n                                                    <Col span={11} offset={1}>\n                                                        <Button type=\"dashed\" style={{height:102,width:102}}>\n                                                            <Icon type=\"file-image\" style={{fontSize:32}} />\n                                                            <br/>\n                                                            <span>示例图</span><Icon type=\"zoom-in\" />\n                                                        </Button>\n                                                    </Col>\n                                                </Row>\n                                            )}\n                                    </Form.Item>\n                                </Col>\n                            </Row>\n                            <Row>\n                                <Col span={24}>\n                                <Form.Item labelCol={{span:24}} wrapperCol={{span:24}}\n                                        label=\"执业证书照片\" help=\"支持扩展名：.jpg .png\">{\n                                            getFieldDecorator(\"person.photo\",{\n                                                rules:[\n                                                    {required:true}\n                                                ]\n                                            })(\n                                                <Row>\n                                                    <Col span={6}>\n                                                        <Upload\n                                                            name=\"person.photo\"\n                                                            listType=\"picture-card\"\n                                                            className=\"avatar-uploader\"\n                                                            showUploadList={false}\n                                                        >\n                                                            <div>\n                                                                <Icon type=\"plus\" />\n                                                                <div className=\"ant-upload-text\">上传</div>\n                                                            </div>\n                                                        </Upload>\n                                                    </Col>\n                                                </Row>\n                                            )}\n                                    </Form.Item>\n                                </Col>\n                            </Row>\n                            <Row>\n                                <Col span={24}>\n                                <Form.Item labelCol={{span:24}} wrapperCol={{span:24}}\n                                        label=\"资格证书照片\" help=\"支持扩展名：.jpg .png\">{\n                                            getFieldDecorator(\"person.photo\",{\n                                                rules:[\n                                                    {required:true}\n                                                ]\n                                            })(\n                                                <Row>\n                                                    <Col span={6}>\n                                                        <Upload\n                                                            name=\"person.photo\"\n                                                            listType=\"picture-card\"\n                                                            className=\"avatar-uploader\"\n                                                            showUploadList={false}\n                                                        >\n                                                            <div>\n                                                                <Icon type=\"plus\" />\n                                                                <div className=\"ant-upload-text\">上传</div>\n                                                            </div>\n                                                        </Upload>\n                                                    </Col>\n                                                </Row>\n                                            )}\n                                    </Form.Item>\n                                </Col>\n                            </Row>\n                            <Row>\n                                <Col span={24}>\n                                <Form.Item labelCol={{span:24}} wrapperCol={{span:24}}\n                                        label=\"职称证书照片\" help=\"支持扩展名：.jpg .png\">{\n                                            getFieldDecorator(\"person.photo\",{\n                                                rules:[\n                                                    {required:true}\n                                                ]\n                                            })(\n                                                <Row>\n                                                    <Col span={6}>\n                                                        <Upload\n                                                            name=\"person.photo\"\n                                                            listType=\"picture-card\"\n                                                            className=\"avatar-uploader\"\n                                                            showUploadList={false}\n                                                        >\n                                                            <div>\n                                                                <Icon type=\"plus\" />\n                                                                <div className=\"ant-upload-text\">上传</div>\n                                                            </div>\n                                                        </Upload>\n                                                    </Col>\n                                                </Row>\n                                            )}\n                                    </Form.Item>\n                                </Col>\n                            </Row>\n                        </Form>\n                        <div className=\"bottom-bar\">\n                            <Button onClick={this.onClose} style={{ marginRight: 8 }}>\n                                取消\n                            </Button>\n                            <Button onClick={this.onClose} type=\"primary\">\n                                保存\n                            </Button>\n                        </div>\n                    </Drawer>\n                ):null}\n            </div>\n        );\n    }\n\n    /**\n     * 组件载入后读取人员列表\n     */\n    componentDidMount=async ()=>{\n        // 获取医护人员信息\n        const staffResponse:HttpResponse<Array<ApplyContentDoctorOrNurse>>=\n            await ((new HttpRequest()).get<Array<ApplyContentDoctorOrNurse>>(\n                `webapi/apply/doctorAndNurse?applyNo=${this.props.serialNumber}`\n            ));\n        // 失败跳转500\n        if(!staffResponse.success||!staffResponse.data){\n            this.props.history.push(PageError500);\n            return;\n        }\n\n        // 设置显示\n        this.setState({\n            ...this.state,\n            data:staffResponse.data\n        });\n    }\n\n    /**\n     * 打开抽屉\n     */\n    showDrawer = (index:number) => {\n        this.setState({\n            drawerVisible: true,\n        });\n    };\n\n    /**\n     * 关闭抽屉\n     */\n    onClose = () => {\n        this.setState({\n            drawerVisible: false,\n        });\n    };\n}\nexport default withRouter(Form.create<StaffDoctorsAndNurseProps>({name:\"doctorsAndNurseForm\"})(StaffDoctorsAndNurse));"]},"metadata":{},"sourceType":"module"}