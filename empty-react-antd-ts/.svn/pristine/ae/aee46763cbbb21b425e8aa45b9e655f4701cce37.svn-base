import {CascaderOptionType} from 'antd/lib/cascader';

/**通用字典格式 */
export type DictionaryData=CascaderOptionType;

/**
 * 数据字典载入器：单例模式；
 * 从本地缓存中载入字典，如果不存在或已过期则从服务端拉取数据字典。
 * （目前直接使用前端默认数据）
 */
export class DictionaryLoader{
    public static readonly instance:DictionaryLoader=new DictionaryLoader();
    private DictionaryLoader(){}

    /**
     * 所属街道
     */
    street():Array<DictionaryData>{return dictionaryStreet;}

    /**
     * 隶属关系
     */
    subjection():Array<DictionaryData>{return dictionarySubjection;}

    /**
     * 所有制性质
     */
    ownership():Array<DictionaryData>{return dictionaryOwnership;}

    /**
     * 大型设备名称
     */
    equipment():Array<DictionaryData>{return dictionaryEquipment;}

    /**
     * 诊疗范围（全部）
     */
    medical():Array<DictionaryData>{return dictionaryMedical;}

    /**
     * 诊疗范围（简略版，用于备案）
     */
    medicalSimple():Array<DictionaryData>{
        const result:Array<DictionaryData>=[];
        dictionaryMedical.map((dict:DictionaryData)=>{
            // 过滤掉只在全部中显示的第一层
            if(dict.tag&&"all"===dict.tag) return;
            const children:Array<DictionaryData>=[];
            if(dict.children){
                dict.children.map((childDict:DictionaryData)=>{
                    // 过滤掉只在全部中显示的第二层
                    if(dict.tag&&"all"===dict.tag) return;
                    children.push(childDict);
                });
            }
            result.push({value:dict.value,label:dict.label,children:children});
        });
        return result;
    }

    /**
     * 学历
     */
    education():Array<DictionaryData>{return dictionaryEducation;}

    /**
     * 职务职称
     */
    job():Array<DictionaryData>{return dictionaryJob;}

    /**
     * 民族
     */
    nation():Array<DictionaryData>{return dictionaryNation;}

    /**
     * 政治面貌
     */
    politics():Array<DictionaryData>{return dictionaryPolitics;}

    /**
     * 服务对象
     */
    serviceObject():Array<DictionaryData>{return dictionaryServiceObject;}

    /**
     * 服务方式
     */
    serviceMode():Array<DictionaryData>{return dictionaryServiceMode;}

    /**
     * 机构类别
     */
    category():Array<DictionaryData>{return dictionaryCategory;}

    /**
     * 机构等级
     */
    level():Array<DictionaryData>{return dictionaryLevel;}

    /**
     * 机构等次
     */
    grade():Array<DictionaryData>{return dictionaryGrade;}
}

