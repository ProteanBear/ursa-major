AA,iBAAiB,GAAE;AACf,aAAQC,MAAR,CAAe;AACXC,MAAAA,GAAG,EAAC,EADO;AAEXC,MAAAA,QAAQ,EAAC;AAFE,KAAf;AAIH;;AAnFiE;;AAqFtE,eAAexB,UAAU,CAACE,mBAAD,CAAzB","sourcesContent":["import './Container.css';\n\nimport React from 'react';\nimport {withRouter, RouteComponentProps} from 'react-router-dom';\nimport { Layout, Menu, Button, Avatar, Dropdown, Icon,message } from 'antd';\nimport ImageViewer from './ImageViewer';\n\n/**\n * 公共组件：顶部栏，包括logo、菜单和登录状态判断；\n */\nclass ContainerWithHeader extends React.Component<RouteComponentProps>{\n    readonly state:{\n    }={\n        \n    }\n\n    /**\n     * 顶部退出登录处理：请求服务后，清除本地缓存，跳转到登录页\n     */\n    logout=()=>{\n        this.props.history.replace(\"/\");\n    };\n\n    /**\n     * 通用布局容器\n     */\n    render(){\n        const {Header,Content,Footer}=Layout;\n        const selectedKey=\"switch\";\n        const isLogin=false;\n        const menu=(\n            <Menu>\n                <Menu.Item onClick={this.logout}>\n                    <Icon type=\"logout\"/> 退出\n                </Menu.Item>\n            </Menu>\n        );\n\n        return (\n            <Layout className=\"layout\">\n                <Header>\n                    <div className=\"header-container\">\n                        <div className=\"header-logo\">{process.env.REACT_APP_NAME}</div>\n                        {isLogin?(\n                                <Dropdown overlay={menu} placement=\"bottomLeft\">\n                                    <Button className=\"header-login-button\" type=\"link\">\n                                        <Avatar icon=\"user\"/> 机构名称\n                                    </Button>\n                                </Dropdown>\n                            ):(\n                                <Button className=\"header-login-button\"\n                                    type=\"link\" onClick={this.logout}>\n                                    <Avatar icon=\"user\"/> 机构登录\n                                </Button>\n                            )\n                        }\n                        {isLogin?(\n                            <Menu theme=\"dark\" mode=\"horizontal\" \n                                defaultSelectedKeys={[selectedKey]}\n                                style={{lineHeight:'64px'}}\n                                >\n                                <Menu.Item key=\"switch\">信息维护</Menu.Item>\n                            </Menu>\n                        ):(\n                            <Menu theme=\"dark\" mode=\"horizontal\" \n                                defaultSelectedKeys={[selectedKey]}\n                                style={{lineHeight:'64px'}}\n                                >\n                            <Menu.Item key=\"switch\">执业申请</Menu.Item>\n                        </Menu>\n                        )\n                        }\n                    </div>\n                </Header>\n                <Content className=\"content-container\">\n                    {this.props.children}\n                </Content>\n                <Footer className=\"footer-container\"></Footer>\n\n                {/* 图片查看器 */}\n                <ImageViewer />\n            </Layout>\n        );\n    }\n\n    /**\n     * 载入全部后，设置消息提示样式\n     */\n    componentDidMount(){\n        message.config({\n            top:70,\n            maxCount:3\n        });\n    }\n}\nexport default withRouter(ContainerWithHeader);"]},"metadata":{},"sourceType":"module"}