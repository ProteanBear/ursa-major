AA,IAAI,EAAC,YAAxB;AACI,MAAA,mBAAmB,EAAE,CAACF,WAAD,CADzB;AAEI,MAAA,KAAK,EAAE;AAACM,QAAAA,UAAU,EAAC;AAAZ,OAFX;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,OAII,0BAAM,IAAN;AAAW,MAAA,GAAG,EAAC,QAAf;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,kCAJJ,CAhBJ,CADJ,CADJ,EA0BI,oBAAC,OAAD;AAAS,MAAA,SAAS,EAAC,mBAAnB;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,OACK,KAAKC,KAAL,CAAWC,QADhB,CA1BJ,EA6BI,oBAAC,MAAD;AAAQ,MAAA,SAAS,EAAC,kBAAlB;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,MA7BJ,CADJ;AAiCH;;AA9C2D","sourcesContent":["import './Container.css';\n\nimport React from 'react';\nimport {NavLink, Link} from 'react-router-dom';\nimport { Layout, Menu, Button, Avatar, Dropdown } from 'antd';\nimport { LoginChecker } from '../app/LoginChecker';\n\n/**\n * 公共组件：顶部栏，包括logo、菜单和登录状态判断；\n */\nexport default class ContainerWithHeader extends React.Component{\n    render(){\n        const {Header,Content,Footer}=Layout;\n        const selectedKey=\"switch\";\n        const isLogin=LoginChecker.isLogin;\n        const menu=(\n            <Menu>\n                <Menu.Item>\n                    <Link to=\"/\">退出登录</Link>\n                </Menu.Item>\n            </Menu>\n        );\n\n        return (\n            <Layout className=\"layout\">\n                <Header>\n                    <div className=\"header-container\">\n                        <div className=\"header-logo\">{process.env.REACT_APP_NAME}</div>\n                        {()=>isLogin?(\n                                <Dropdown overlay={menu} placement=\"bottomLeft\">\n                                    <Button className=\"header-login-button\" type=\"link\">\n                                        <Avatar icon=\"user\"/> 机构名称\n                                    </Button>\n                                </Dropdown>\n                            ):(\n                                <Button className=\"header-login-button\"\n                                    type=\"link\"\n                                >\n                                    <Link to=\"/\"><Avatar icon=\"user\"/> 机构登录</Link>\n                                </Button>\n                            )\n                        }\n                        <Menu theme=\"dark\" mode=\"horizontal\" \n                            defaultSelectedKeys={[selectedKey]}\n                            style={{lineHeight:'64px'}}\n                            >\n                            <Menu.Item key=\"switch\">执业申请</Menu.Item>\n                        </Menu>\n                    </div>\n                </Header>\n                <Content className=\"content-container\">\n                    {this.props.children}\n                </Content>\n                <Footer className=\"footer-container\"></Footer>\n            </Layout>\n        );\n    }\n}"]},"metadata":{},"sourceType":"module"}