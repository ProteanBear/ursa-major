m, Input, Radio, Select, Cascader, Upload, Avatar } from 'antd';\nimport { DictionaryLoader, DictionaryData } from '../app/DictionaryLoader';\nimport { FormComponentProps } from 'antd/lib/form';\nimport { ApplyContentDoctorOrNurse } from '../app/Model';\n\ntype StaffDoctorsAndNurseProps=FormComponentProps&{\n    /**人员列表 */\n    staffs:Array<ApplyContentDoctorOrNurse>\n}\n\n/**\n * 医护人员管理组件\n */\nclass StaffDoctorsAndNurse extends React.Component<StaffDoctorsAndNurseProps>{\n    state:{drawerVisible:boolean}= { \n        drawerVisible: false \n    };\n\n    /**\n     * 页面布局\n     */\n    render(){\n        const {getFieldDecorator} = this.props.form;\n        const {Meta}=Card;\n        const {Option}=Select;\n\n        const {staffs}=this.props;\n        const education=DictionaryLoader.instance.education();\n        const job=DictionaryLoader.instance.job();\n\n        return (\n            <div>\n                <Row>\n                    <Col span={4}>\n                        <Card\n                            actions={[\n                                <Icon type=\"edit\" key=\"edit\" />,\n                                <Icon type=\"delete\" key=\"delete\" />\n                            ]}\n                        >\n                            <Meta\n                                avatar={<Avatar src=\"https://gw.alipayobjects.com/zos/rmsportal/JiqGstEfoWAOHiTxclqi.png\"/>}\n                                title=\"姓名\"\n                                description=\"主治中医师\"\n                            />\n                        </Card>\n                    </Col>\n                    <Col span={3} offset={1}>\n                        <Button type=\"dashed\" \n                            style={{width:\"100%\",height:173}}\n                            onClick={this.showDrawer}\n                        >\n                            <Icon type=\"plus\" /><br/>\n                            添加\n                        </Button>\n                    </Col>\n                </Row>\n                \n                {/* 医生和护士编辑抽屉 */}\n                <Drawer\n                    title=\"添加医护人员\" width={560}\n                    onClose={this.onClose}\n                    visible={this.state.drawerVisible}\n                >\n                    <Form labelCol={{span:6}} wrapperCol={{span:18}}>\n                        <h4 className=\"title-form-sub\">信息填写</h4>\n                        <Row>\n                            <Col span={15}>\n                                <Form.Item label=\"人员姓名\">{\n                                    getFieldDecorator(\"person.name\",{\n                                        rules:[\n                                            {required:true}\n                                        ]\n                                    })(<Input placeholder=\"请输入\" required={true}/>)}\n                                </Form.Item>\n                            </Col>\n                            <Col span={9}>\n                                <Form.Item label=\"性别\" \n                                    labelCol={{span:8}} wrapperCol={{span:16}}\n                                >\n                                    <Radio.Group value={1}>\n                                        <Radio value={1}>男</Radio>\n                                        <Radio value={2}>女</Radio>\n                                    </Radio.Group>\n                                </Form.Item>\n                            </Col>\n                        </Row>\n                        <Row>\n                            <Col span={15}>\n                                <Form.Item label=\"身份证号\">{\n                                    getFieldDecorator(\"person.identity\",{\n                                        rules:[\n                                            {required:true}\n                                        ]\n                                    })(<Input placeholder=\"请输入\" required={true}/>)}\n                                </Form.Item>\n                            </Col>\n                            <Col span={9}>\n                                <Form.Item label=\"年龄\" \n                                    labelCol={{span:8}} wrapperCol={{span:16}}\n                                >\n                                    <span className=\"ant-form-text\">30岁</span>\n                                </Form.Item>\n                            </Col>\n                        </Row>\n                        <Row>\n                            <Col span={15}>\n                                <Form.Item label=\"毕业院校\">\n                                    <Input placeholder=\"请输入\" required={true}/>\n                                </Form.Item>\n                            </Col>\n                            <Col span={9}>\n                                <Form.Item label=\"学历\" \n                                    labelCol={{span:8}} wrapperCol={{span:16}}\n                                >\n                                    <Select showSearch\n                                        placeholder=\"请选择\"\n                                        optionFilterProp=\"children\"\n                                        filterOption={(input, option:React.ReactElement) =>\n                                            option.props.children.toLowerCase().indexOf(input.toLowerCase()) >= 0\n                                        }\n                                    >\n                                        {education.map((data:DictionaryData,index:number)=>\n                                            <Option key={data.value}>{data.label}</Option>\n                                        )}\n                                    </Select>\n                                </Form.Item>\n                            </Col>\n                        </Row>\n                        <Row>\n                            <Col span={15}>\n                                <Form.Item label=\"职务职称\">{\n                                    getFieldDecorator(\"person.job\",{\n                                        rules:[\n                                            {required:false}\n                                        ]\n                                    })(<Cascader options={job} placeholder=\"请选择\" />)}\n                                </Form.Item>\n                            </Col>\n                            <Col span={9}>\n                                <Form.Item label=\"科室\"\n                                    labelCol={{span:8}} wrapperCol={{span:16}}\n                                >\n                                    <Input placeholder=\"请输入\" required={true}/>\n                                </Form.Item>\n                            </Col>\n                        </Row>\n                        <h4 className=\"title-form-sub\">上传内容</h4>\n                        <Row>\n                            <Col span={12}>\n                                <Form.Item labelCol={{span:24}} wrapperCol={{span:24}}\n                                    label=\"身份证正面\" help=\"支持扩展名：.jpg .png\">{\n                                        getFieldDecorator(\"person.identity.front\",{\n                                            rules:[\n                                                {required:true}\n                                            ]\n                                        })(\n                                            <Row>\n                                                <Col span={10}>\n                                                    <Upload\n                                                        name=\"person.identity.front\"\n                                                        listType=\"picture-card\"\n                                                        className=\"avatar-uploader\"\n                                                        showUploadList={false}\n                                                    >\n                                                        <div>\n                                                            <Icon type=\"plus\" />\n                                                            <div className=\"ant-upload-text\">上传</div>\n                                                        </div>\n                                                    </Upload>\n                                                </Col>\n                                                <Col span={11} offset={1}>\n                                                    <Button type=\"dashed\" style={{height:102,width:102}}>\n                                                        <Icon type=\"file-image\" style={{fontSize:32}} />\n                                                        <br/>\n                                                        <span>示例图</span><Icon type=\"zoom-in\" />\n                                                    </Button>\n                                                </Col>\n                                            </Row>\n                                        )}\n                                </Form.Item>\n                            </Col>\n                            <Col span={12}>\n                                <Form.Item labelCol={{span:24}} wrapperCol={{span:24}}\n                                    label=\"身份证背面\" help=\"支持扩展名：.jpg .png\">{\n                                        getFieldDecorator(\"person.identity.back\",{\n                                            rules:[\n                                                {required:true}\n                                            ]\n                                        })(\n                                            <Row>\n                                                <Col span={10}>\n                                                    <Upload\n                                                        name=\"person.identity.back\"\n                                                        listType=\"picture-card\"\n                                                        className=\"avatar-uploader\"\n                                                        showUploadList={false}\n                                                    >\n                                                        <div>\n                                                            <Icon type=\"plus\" />\n                                                            <div className=\"ant-upload-text\">上传</div>\n                                                        </div>\n                                                    </Upload>\n                                                </Col>\n                                                <Col span={11} offset={1}>\n                                                    <Button type=\"dashed\" style={{height:102,width:102}}>\n                                                        <Icon type=\"file-image\" style={{fontSize:32}} />\n                                                        <br/>\n                                                        <span>示例图</span><Icon type=\"zoom-in\" />\n                                                    </Button>\n                                                </Col>\n                                            </Row>\n                                        )}\n                                </Form.Item>\n                            </Col>\n                        </Row>\n                        <Row>\n                            <Col span={12}>\n                                <Form.Item labelCol={{span:24}} wrapperCol={{span:24}}\n                                    label=\"二寸寸照\" help=\"支持扩展名：.jpg .png\">{\n                                        getFieldDecorator(\"person.photo\",{\n                                            rules:[\n                                                {required:true}\n                                            ]\n                                        })(\n       