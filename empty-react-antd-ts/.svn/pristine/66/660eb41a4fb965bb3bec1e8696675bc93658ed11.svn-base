const dictionaryMedicalMap=(()=>{
    //第一层的Map
    const resultMap=new Map<string,string>();
    dictionaryMedical.forEach((data)=>{
        if(!data.value||!data.label) return;
        resultMap.set(data.value,String(data.label));
        if(data.children){
            data.children.forEach((subData)=>{
                if(!subData.value||!subData.label) return;
                resultMap.set(subData.value,String(subData.label));
            })
        }
    });
    return resultMap;
})()
