/**机构类型Map（通过Key获取名称） */
const dictionaryCategoryMap=(()=>{
    //第一层的Map
    const resultMap=new Map<string,string>();
    dictionaryCategory.forEach((data)=>{
        if(!data.value||!data.label) return;
        if(data.children){
            data.children.forEach((subData)=>{
                if(!subData.value||!subData.label) return;
                if(subData.children){
                    subData.children.forEach((lastData)=>{
                        if(!lastData.value||!lastData.label) return;
                        resultMap.set(lastData.value,String(lastData.label));
                    })
                }
            })
        }
    });
    return resultMap;
})()
/**机构类型子级倒转Map（通过Key获取级联选项数组） */
const dictionaryCategoryParentArray=(()=>{
    //第一层的Map
    const resultMap=new Map<string,Array<string>>();
    dictionaryCategory.forEach((data)=>{
        if(data.children){
            data.children.forEach((subData)=>{
                if(subData.children){
                    subData.children.forEach((lastData)=>{
                        if(!data.value||!subData.value||!lastData.value) return;
                        resultMap.set(lastData.value,[data.value,subData.value,lastData.value]);
                    })
                }
            })
        }
    });
    return resultMap;
})()
