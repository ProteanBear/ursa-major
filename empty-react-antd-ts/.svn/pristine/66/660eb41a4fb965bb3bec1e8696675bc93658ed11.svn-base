import {CascaderOptionType} from 'antd/lib/cascader';
import { fsyncSync } from 'fs';

/**通用字典格式 */
export type DictionaryData=CascaderOptionType;

/**
 * 数据字典载入器：单例模式；
 * 从本地缓存中载入字典，如果不存在或已过期则从服务端拉取数据字典。
 * （目前直接使用前端默认数据）
 */
export class DictionaryLoader{
    public static readonly instance:DictionaryLoader=new DictionaryLoader();
    private DictionaryLoader(){}

    /**
     * 所属街道
     */
    street():Array<DictionaryData>{return dictionaryStreet;}

    /**
     * 所属街道
     */
    streetLabel(key:string):string{return dictionaryStreetMap.get(key)||"";}

    /**
     * 隶属关系
     */
    subjection():Array<DictionaryData>{return dictionarySubjection;}

    /**
     * 隶属关系
     */
    subjectionLabel(key:string):string{return dictionarySubjectionMap.get(key)||"";}

    /**
     * 所有制形式
     */
    ownership():Array<DictionaryData>{return dictionaryOwnership;}

    /**
     * 所有制形式
     */
    ownershipLabel(key:string):string{return dictionaryOwnershipMap.get(key)||"";}

    /**
     * 大型设备名称
     */
    equipment():Array<DictionaryData>{return [...dictionaryEquipment];}

    /**
     * 诊疗范围（全部）
     */
    medical():Array<DictionaryData>{return dictionaryMedical;}

    /**
     * 根据标识找到显示名称
     */
    medicalLabel(key:string):string{return dictionaryMedicalMap.get(key)||"";}

    /**
     * 诊疗范围（简略版，用于备案）
     */
    medicalSimple():Array<DictionaryData>{
        const result:Array<DictionaryData>=[];
        dictionaryMedical.forEach((dict:DictionaryData)=>{
            // 过滤掉只在全部中显示的第一层
            if(dict.tag&&"all"===dict.tag) return;
            const children:Array<DictionaryData>=[];
            if(dict.children){
                dict.children.forEach((childDict:DictionaryData)=>{
                    // 过滤掉只在全部中显示的第二层
                    if(dict.tag&&"all"===dict.tag) return;
                    children.push(childDict);
                });
            }
            result.push({value:dict.value,label:dict.label,children:children});
        });
        return result;
    }

    /**
     * 学历
     */
    education():Array<DictionaryData>{return dictionaryEducation;}

    /**
     * 学历
     */
    educationLabel(key:string):string{return dictionaryEducationMap.get(key)||"";}

    /**
     * 职务职称
     */
    job(tags?:Array<string>):Array<DictionaryData>{
        if(!tags) return dictionaryJob;

        const result:Array<DictionaryData>=[];
        const tagSet=new Set([...tags]);
        dictionaryJob.forEach((dict:DictionaryData)=>{
            if(dict.tag&&tagSet.has(dict.tag)){
                result.push(dict);
            }
        });
        return result;
    }

    /**
     * 根据标识找到显示名称
     */
    jobLabel(key:string):string{return dictionaryJobMap.get(key)||"";}

    /**
     * 民族
     */
    nation():Array<DictionaryData>{return dictionaryNation;}

    /**
     * 民族
     */
    nationLabel(key:string):string{return dictionaryNationMap.get(key)||"";}

    /**
     * 政治面貌
     */
    politics():Array<DictionaryData>{return dictionaryPolitics;}

    /**
     * 政治面貌
     */
    politicsLabel(key:string):string{return dictionaryPoliticsMap.get(key)||"";}

    /**
     * 服务对象
     */
    serviceObject():Array<DictionaryData>{return dictionaryServiceObject;}

    /**
     * 服务方式
     */
    serviceMode():Array<DictionaryData>{return dictionaryServiceMode;}

    /**
     * 机构类别
     */
    category():Array<DictionaryData>{return dictionaryCategory;}

    /**
     * 机构类别
     */
    categoryLabel(key:string):string{return dictionaryCategoryMap.get(key)||"";}

    /**
     * 机构类别
     */
    categoryParentArray(key:string):Array<string>{return dictionaryCategoryParentArray.get(key)||[];}

    /**
     * 机构等级
     */
    level():Array<DictionaryData>{return dictionaryLevel;}

    /**
     * 机构等次
     */
    grade():Array<DictionaryData>{return dictionaryGrade;}
}

