K,EAAE;AAACE,QAAAA,QAAQ,EAAC;AAAV,OAA/B;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,MADJ,EAEI;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,MAFJ,EAGI;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,4BAHJ,EAGoB;AAAM,MAAA,IAAI,EAAC,SAAX;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,MAHpB,CADJ,CAfJ,CALJ,CAFR,CADJ,CAnEJ,CAzFJ,EA+LI;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,OACI;AAAK,MAAA,IAAI,EAAE,EAAX;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,OACI,0BAAM,IAAN;AAAW,MAAA,KAAK,EAAC,0BAAjB;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,OACI,oBAAC,eAAD;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,MADJ,CADJ,CADJ,CA/LJ,CADJ;AAyMH;;AAvN4D;;AAyNjE,eAAe,MAAKC,MAAL,CAAY;AAAClB,EAAAA,IAAI,EAAC;AAAN,CAAZ,EAAqCb,cAArC,CAAf","sourcesContent":["import React from 'react';\nimport { Form, Col, Input, Radio, DatePicker, Select, Cascader, Upload, Icon, Row, Button } from 'antd';\nimport { FormComponentProps } from 'antd/lib/form';\nimport { DictionaryLoader, DictionaryData } from '../app/DictionaryLoader';\nimport ApplyFormResume from './ApplyFormResume';\nimport { ApplyContentManager } from '../app/Model';\n\ntype ApplyFormLegalProps=FormComponentProps&{\n    manager:ApplyContentManager\n}\n\n/**\n * 申请公用表单：法人相关信息\n */\nclass ApplyFormLegal extends React.Component<ApplyFormLegalProps>{\n    readonly state:{}={}\n\n    /**\n     * 页面布局\n     */\n    render(){\n        const {getFieldDecorator} = this.props.form;\n        const {Option}=Select;\n\n        const {manager}=this.props;\n        const education=DictionaryLoader.instance.education();\n        const job=DictionaryLoader.instance.job();\n\n        return (\n            <Form layout=\"vertical\">\n                <Row>\n                    <Col span={8}>\n                        <Form.Item label=\"姓名\">{\n                            getFieldDecorator(\"name\",{\n                                initialValue:manager.name,\n                                rules:[\n                                    {required:true,message:\"姓名不能为空！\"}\n                                ]\n                            })(<Input placeholder=\"请输入\"/>)}\n                        </Form.Item>\n                    </Col>\n                    <Col span={7} offset={1}>\n                        <Form.Item label=\"性别\">{\n                            getFieldDecorator(\"sex\",{\n                                initialValue:(\"\"===manager.sex?\"男\":manager.sex),\n                                rules:[\n                                    {required:false}\n                                ]\n                            })(\n                                <Radio.Group>\n                                    <Radio value=\"男\">男</Radio>\n                                    <Radio value=\"女\">女</Radio>\n                                </Radio.Group>\n                            )}\n                        </Form.Item>\n                    </Col>\n                </Row>\n                <Row>\n                    <Col span={8}>\n                        <Form.Item label=\"出生年月\">{\n                            getFieldDecorator(\"birthday\",{\n                                rules:[\n                                    {required:false}\n                                ]\n                            })(<DatePicker placeholder=\"请选择日期\" />)}\n                        </Form.Item>\n                    </Col>\n                    <Col span={7} offset={1}>\n                        <Form.Item label=\"学历\">{\n                            getFieldDecorator(\"education\",{\n                                rules:[\n                                    {required:false}\n                                ]\n                            })(\n                                <Select showSearch\n                                    placeholder=\"请选择\"\n                                    optionFilterProp=\"children\"\n                                    filterOption={(input, option:React.ReactElement) =>\n                                        option.props.children.toLowerCase().indexOf(input.toLowerCase()) >= 0\n                                    }\n                                >\n                                    {education.map((data:DictionaryData,index:number)=>{\n                                        return (<Option key={data.value}>{data.label}</Option>);\n                                    })}\n                                </Select>\n                            )}\n                        </Form.Item>\n                    </Col>\n                    <Col span={7} offset={1}>\n                        <Form.Item label=\"毕业专业\">{\n                            getFieldDecorator(\"profession\",{\n                                rules:[\n                                    {required:false}\n                                ]\n                            })(<Input placeholder=\"请输入\"/>)}\n                        </Form.Item>\n                    </Col>\n                </Row>\n                <Row>\n                    <Col span={8}>\n                        <Form.Item label=\"职务职称\">{\n                            getFieldDecorator(\"legal.job\",{\n                                rules:[\n                                    {required:false}\n                                ]\n                            })(<Cascader options={job} placeholder=\"请选择\" />)}\n                        </Form.Item>\n                    </Col>\n                    <Col span={7} offset={1}>\n                        <Form.Item label=\"身份证号\">{\n                            getFieldDecorator(\"legal.identity\",{\n                                rules:[\n                                    {required:true}\n                                ]\n                            })(<Input placeholder=\"请输入\"/>)}\n                        </Form.Item>\n                    </Col>\n                </Row>\n                <Row>\n                    <Col span={8}>\n                        <Form.Item\n                            label=\"签字表\" help=\"支持扩展名：.jpg .png\">{\n                                getFieldDecorator(\"legal.signature\",{\n                                    rules:[\n                                        {required:true}\n                                    ]\n                                })(\n                                    <Row>\n                                        <Col span={8}>\n                                            <Upload\n                                                name=\"legal.signature\"\n                                                listType=\"picture-card\"\n                                                className=\"avatar-uploader\"\n                                                showUploadList={false}\n\n                                            >\n                                                <div>\n                                                    <Icon type=\"plus\" />\n                                                    <div className=\"ant-upload-text\">上传</div>\n                                                </div>\n                                            </Upload>\n                                        </Col>\n                                        <Col span={8}>\n                                            <Button type=\"dashed\" style={{width:102,height:102}}>\n                                                <Icon type=\"download\" /><br/>\n                                                签字表模板\n                                            </Button>\n                                        </Col>\n                                    </Row>\n                                )}\n                        </Form.Item>\n                    </Col>\n                    <Col span={7} offset={1}>\n                        <Form.Item\n                            label=\"身份证正面\" help=\"支持扩展名：.jpg .png\">{\n                                getFieldDecorator(\"legal.identity.front\",{\n                                    rules:[\n                                        {required:true}\n                                    ]\n                                })(\n                                    <Row>\n                                        <Col span={8}>\n                                            <Upload\n                                                name=\"legal.identity.front\"\n                                                listType=\"picture-card\"\n                                                className=\"avatar-uploader\"\n                                                showUploadList={false}\n                                            >\n                                                <div>\n                                                    <Icon type=\"plus\" />\n                                                    <div className=\"ant-upload-text\">上传</div>\n                                                </div>\n                                            </Upload>\n                                        </Col>\n                                        <Col span={8} offset={1}>\n                                            <Button type=\"dashed\" style={{height:102,width:102}}>\n                                                <Icon type=\"file-image\" style={{fontSize:32}} />\n                                                <br/>\n                                                <span>示例图</span><Icon type=\"zoom-in\" />\n                                            </Button>\n                                        </Col>\n                                    </Row>\n                                )}\n                        </Form.Item>\n                    </Col>\n                    <Col span={7} offset={1}>\n                        <Form.Item\n                            label=\"身份证背面\" help=\"支持扩展名：.jpg .png\">{\n                                getFieldDecorator(\"legal.identity.back\",{\n                                    rules:[\n                                        {required:true}\n                                    ]\n                                })(\n                                    <Row>\n                                        <Col span={8}>\n                                            <Upload\n                                                name=\"legal.identity.back\"\n                                                listType=\"picture-card\"\n                                                className=\"avatar-uploader\"\n                                                showUploadList={false}\n\n                                            >\n                                                <div>\n                                                    <Icon type=\"plus\" />\n                                                    <div className=\"ant-upload-text\">上传</div>\n                                                </div>\n                                            </Upload>\n                                        </Col>\n                                        <Col span={8} offset={1}>\n                                            <Button type=\"dashed\" style={{height:102,width:102}}>\n                                                <Icon type=\"file-image\" style={{fontSize:32}} />\n                                                <br/>\n                                                <span>示例图</span><Icon type=\"zoom-in\" />\n                                            </Button>\n                                        </Col>\n                                    </Row>\n                                )}\n                        </Form.Item>\n                    </Col>\n                </Row>\n                <Row>\n                    <Col span={24}>\n                        <Form.Item label=\"工作履历\">\n                            <ApplyFormResume />\n                        </Form.Item>\n                    </Col>\n                </Row>\n            </Form>\n        );\n    }\n}\nexport default Form.create({name:\"applyFormLegal\"})(ApplyFormLegal);"]},"metadata":{},"sourceType":"module"}