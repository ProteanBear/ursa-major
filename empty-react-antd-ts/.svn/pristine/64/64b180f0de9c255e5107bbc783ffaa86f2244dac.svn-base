GAAG,EAAC,0BAAb;AAAoB,MAAA,GAAG,EAAC,iBAAxB;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,OACI;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,MADJ,CAVJ,EAaI,oBAAC,OAAD;AAAS,MAAA,GAAG,EAAC,0BAAb;AAAoB,MAAA,GAAG,EAAC,YAAxB;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,OACI;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,MADJ,CAbJ,EAgBI,oBAAC,OAAD;AAAS,MAAA,GAAG,EAAC,0BAAb;AAAoB,MAAA,GAAG,EAAC,aAAxB;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,OACI;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,MADJ,CAhBJ,CADJ,CADJ,CA1BJ,CADJ;AAqDH;AAED;;;;;AAGA,QAAMC,iBAAN,GAAyB;AACrB;AACA,UAAMC,cAAc,GAAC,MAAQ,IAAI3B,WAAJ,EAAD,CACvB4B,GADuB,CACD,mBADC,CAA5B,CAFqB,CAKrB;;AACA,QAAG,CAACD,cAAc,CAACE,OAAhB,IAAyB,CAACF,cAAc,CAACG,IAA5C,EAAiD;AAC7C,WAAKC,KAAL,CAAWC,OAAX,CAAmBC,IAAnB,CAAwBlC,YAAxB;AACA;AACH,KAToB,CAWrB;;;AACA,QAAIY,KAAJ,EAAUC,QAAV,EAAmBC,OAAnB;AAZqB,UAadqB,aAbc,GAaCP,cAAc,CAACG,IAbhB,CAadI,aAbc;AAcrBA,IAAAA,aAAa,CAACC,OAAd,CAAsBC,MAAM,IAAE;AAC1B,cAAOA,MAAM,CAACC,IAAd;AACI;AACA,aAAK,CAAL;AACI1B,UAAAA,KAAK,GAACyB,MAAN;AACA;AACJ;;AACA,aAAK,CAAL;AACIxB,UAAAA,QAAQ,GAACwB,MAAT;AACA;AACJ;;AACA,aAAK,CAAL;AACIvB,UAAAA,OAAO,GAACuB,MAAR;AACA;;AACJ;AAbJ;AAeH,KAhBD,EAdqB,CAgCrB;;AACA,SAAKE,QAAL,mBACO,KAAKjC,KADZ;AAEIK,MAAAA,YAAY,EAACiB,cAAc,CAACG,IAAf,CAAoBS,OAFrC;AAGId,MAAAA,UAAU,EAACE,cAAc,CAACG,IAAf,CAAoBU,SAHnC;AAII7B,MAAAA,KAAK,EAACA,KAJV;AAKIC,MAAAA,QAAQ,EAACA,QALb;AAMIC,MAAAA,OAAO,EAACA;AANZ;AAQH;;AAlI0D;;AAoI/D,eAAeZ,UAAU,CAACE,YAAD,CAAzB","sourcesContent":["import React from 'react';\nimport Container, { ContainerLoading } from '../../components/Container';\nimport { Breadcrumb, Icon,Typography, Row, Col, Descriptions, Statistic, Tabs } from 'antd';\nimport { ApplyContentOrganization, ApplyContentManager, ApplyContentDoctorOrNurse, ApplyContentStaffTech, ApplyContentStaffOther, ApplyDetailGroup, PageError500, ApplyContentEquipment } from '../../app/Model';\nimport { HttpRequest } from '../../app/Request';\nimport { RouteComponentProps, withRouter } from 'react-router';\nimport ApplyDetailInforOrganization from '../../components/ApplyDetailInforOrganization';\n\n/**\n * 机构信息维护首页\n */\nclass Organization extends React.Component<RouteComponentProps>{\n    readonly state:{\n        organization?:ApplyContentOrganization;\n        equipments?:Array<ApplyContentEquipment>\n        legal?:ApplyContentManager;\n        princpal?:ApplyContentManager;\n        contact?:ApplyContentManager;\n        doctorsAndNurses:Array<ApplyContentDoctorOrNurse>;\n        staffTechs:Array<ApplyContentStaffTech>;\n        staffOthers:Array<ApplyContentStaffOther>;\n    }={\n        doctorsAndNurses:[],\n        staffTechs:[],\n        staffOthers:[],\n    }\n\n    /**\n     * 页面布局\n     */\n    render(){\n        if(!this.state.organization\n            ||!this.state.legal\n            ||!this.state.princpal\n            ||!this.state.contact){\n            return (<ContainerLoading />);\n        }\n\n        const {TabPane}=Tabs;\n        const {organization,legal,princpal,contact}=this.state;\n        const {doctorsAndNurses,staffTechs,staffOthers}=this.state;\n\n        return (\n            <Container needLogin={true} selectKey=\"organization\">\n                {/* 标题 */}\n                <Breadcrumb className=\"breadcrumb\">\n                    <Breadcrumb.Item><Icon type=\"home\"/><span>首页</span></Breadcrumb.Item>\n                </Breadcrumb>\n                {/* 基本信息 */}\n                <Row className=\"content-container content-container-des\" style={{marginTop:0,minHeight:0,marginBottom:0,width:\"100%\"}}>\n                    <Col span={16}>\n                        <Descriptions title={organization.name}>\n                            <Descriptions.Item label=\"证书有效期\">{organization.effectiveTimeStr}</Descriptions.Item>\n                            <Descriptions.Item label=\"下个校验日\" span={2}>{organization.nextValidDate}</Descriptions.Item>\n                            <Descriptions.Item label=\"法人\">{legal.name}</Descriptions.Item>\n                            <Descriptions.Item label=\"负责人\">{princpal.name}</Descriptions.Item>\n                            <Descriptions.Item label=\"联系人\">{contact.name}</Descriptions.Item>\n                            <Descriptions.Item label=\"联系电话\" span={3}>{contact.phone}</Descriptions.Item>\n                            <Descriptions.Item span={3} label=\"机构地址\">{organization.address}</Descriptions.Item>\n                        </Descriptions>\n                    </Col>\n                    <Col span={8}>\n                        <div className=\"statistic-group\">\n                            <Statistic title=\"医护人员\" value={doctorsAndNurses.length} />\n                            <Statistic title=\"医技人员\" value={staffTechs.length}/>\n                            <Statistic title=\"其他人员\" value={staffOthers.length}/>\n                        </div>\n                    </Col>\n                </Row>\n                <Row>\n                    <Col span={22} offset={1}>\n                        <Tabs>\n                            <TabPane tab=\"机构详情\" key=\"organization\">\n                                <ApplyDetailInforOrganization \n                                    data={this.state.organization}\n                                    equipments={this.state.equipments}\n                                />\n                            </TabPane>\n                            <TabPane tab=\"管理人员\" key=\"manager\">\n                                <h1></h1>\n                            </TabPane>\n                            <TabPane tab=\"医护人员\" key=\"doctorsAndNurse\">\n                                <h1></h1>\n                            </TabPane>\n                            <TabPane tab=\"医技人员\" key=\"staffTechs\">\n                                <h1></h1>\n                            </TabPane>\n                            <TabPane tab=\"其他人员\" key=\"staffOthers\">\n                                <h1></h1>\n                            </TabPane>\n                        </Tabs>\n                    </Col>\n                </Row>\n            </Container>\n        );\n    }\n\n    /**\n     * 页面载入后，载入机构信息\n     */\n    async componentDidMount(){\n        // 请求\n        const detailResponse=await ((new HttpRequest())\n            .get<ApplyDetailGroup>(\"web/audit/orgInfo\"));\n\n        // 获取失败，跳转500\n        if(!detailResponse.success||!detailResponse.data){\n            this.props.history.push(PageError500);\n            return;\n        }\n\n        // 分解三个管理人员\n        let legal,princpal,contact;\n        const {managerPerson}=detailResponse.data;\n        managerPerson.forEach(person=>{\n            switch(person.type){\n                // 法人\n                case 1:\n                    legal=person;\n                    break;\n                // 负责人\n                case 2:\n                    princpal=person;\n                    break;\n                // 联系人\n                case 3:\n                    contact=person;\n                    break;\n                default:\n            }\n        });\n\n        // 设置内容\n        this.setState({\n            ...this.state,\n            organization:detailResponse.data.orgInfo,\n            equipments:detailResponse.data.equipment,\n            legal:legal,\n            princpal:princpal,\n            contact:contact,\n        });\n    }\n}\nexport default withRouter(Organization);"]},"metadata":{},"sourceType":"module"}