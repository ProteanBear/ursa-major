                  <Spin className=\"card-loading\" size=\"large\" tip=\"载入中...\" />\n                </Col>\n            </Row>);\n        }\n\n        const {getFieldDecorator} = this.props.form;\n        const {Meta}=Card;\n        const {Option}=Select;\n\n        const {data}=this.state;\n        const education=DictionaryLoader.instance.education();\n        const job=DictionaryLoader.instance.job();\n\n        return (\n            <div>\n                <Row>\n                    {data.map((person:ApplyContentDoctorOrNurse)=>(\n                        <Col span={5}>\n                            <Card\n                                actions={[\n                                    <Icon type=\"edit\" key=\"edit\" />,\n                                    <Icon type=\"delete\" key=\"delete\" />\n                                ]}\n                            >\n                                <Meta className=\"staff-meta\"\n                                    avatar={<img src={person.img}/>}\n                                    title={person.name}\n                                    description={<p>{person.position}<br/>{person.title}<br/>{person.dept}</p>}\n                                />\n                            </Card>\n                        </Col>\n                    ))}\n                    <Col span={5} offset={1}>\n                        <Button type=\"dashed\" \n                            style={{width:\"100%\",height:143}}\n                            onClick={this.showDrawer}\n                        >\n                            <Icon type=\"plus\" /><br/>\n                            添加\n                        </Button>\n                    </Col>\n                </Row>\n                \n                {/* 医生和护士编辑抽屉 */}\n                <Drawer\n                    title=\"添加医护人员\" width={560}\n                    onClose={this.onClose}\n                    visible={this.state.drawerVisible}\n                >\n                    <Form labelCol={{span:6}} wrapperCol={{span:18}}>\n                        <h4 className=\"title-form-sub\">信息填写</h4>\n                        <Row>\n                            <Col span={15}>\n                                <Form.Item label=\"人员姓名\">{\n                                    getFieldDecorator(\"person.name\",{\n                                        rules:[\n                                            {required:true}\n                                        ]\n                                    })(<Input placeholder=\"请输入\" required={true}/>)}\n                                </Form.Item>\n                            </Col>\n                            <Col span={9}>\n                                <Form.Item label=\"性别\" \n                                    labelCol={{span:8}} wrapperCol={{span:16}}\n                                >\n                                    <Radio.Group value={1}>\n                                        <Radio value={1}>男</Radio>\n                                        <Radio value={2}>女</Radio>\n                                    </Radio.Group>\n                                </Form.Item>\n                            </Col>\n                        </Row>\n                        <Row>\n                            <Col span={15}>\n                                <Form.Item label=\"身份证号\">{\n                                    getFieldDecorator(\"person.identity\",{\n                                        rules:[\n                                            {required:true}\n                                        ]\n                                    })(<Input placeholder=\"请输入\" required={true}/>)}\n                                </Form.Item>\n                            </Col>\n                            <Col span={9}>\n                                <Form.Item label=\"年龄\" \n                                    labelCol={{span:8}} wrapperCol={{span:16}}\n                                >\n                 