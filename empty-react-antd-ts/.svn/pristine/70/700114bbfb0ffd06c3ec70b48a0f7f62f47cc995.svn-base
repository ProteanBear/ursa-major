NJ,CADJ,CADJ,CADJ,EAeI;AAAK,MAAA,IAAI,EAAE,CAAX;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,cAfJ,CA1FJ,EA2GI;AAAI,MAAA,SAAS,EAAC,gBAAd;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,kCA3GJ,EA4GI;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,OACI;AAAK,MAAA,IAAI,EAAE,EAAX;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,OACK,KAAKtF,gBAAL,CAAsB,eAAtB,EAAsC,OAAtC,EAA8C,IAA9C,EAAmD,KAAnD,CADL,CADJ,EAII;AAAK,MAAA,IAAI,EAAE,EAAX;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,OACK,KAAKA,gBAAL,CAAsB,cAAtB,EAAqC,OAArC,EAA6C,IAA7C,EAAkD,KAAlD,CADL,CAJJ,CA5GJ,EAoHI;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,OACI;AAAK,MAAA,IAAI,EAAE,EAAX;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,OACK,KAAKA,gBAAL,CAAsB,eAAtB,EAAsC,MAAtC,EAA6C,KAA7C,CADL,CADJ,CApHJ,CAZJ,EAsII;AAAK,MAAA,SAAS,EAAC,YAAf;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,OACI;AAAQ,MAAA,OAAO,EAAE,KAAK8B,OAAtB;AAA+B,MAAA,KAAK,EAAE;AAAE4E,QAAAA,WAAW,EAAE;AAAf,OAAtC;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,sBADJ,EAII;AAAQ,MAAA,OAAO,EAAE,KAAK3C,SAAtB;AAAiC,MAAA,IAAI,EAAC,SAAtC;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,sBAJJ,CAtIJ,CADQ,GAgJV,IAvLN,CADJ;AA2LH;AAED;;;;;AA3QqD;;AAsjBzD,eAAe5F,UAAU,CAAC,MAAK8C,MAAL,CAA6B;AAACoB,EAAAA,IAAI,EAAC;AAAN,CAA7B,EAAiD/D,UAAjD,CAAD,CAAzB","sourcesContent":["import React from 'react';\nimport { Row, Col, Card, Button, Icon, Drawer, Form, Input, Radio, Select, Cascader, Upload, Spin, Modal } from 'antd';\nimport { DictionaryLoader, DictionaryData } from '../app/DictionaryLoader';\nimport { ApplyContentStaffOther, ApplyJobAndTitleTag, AppComponentProps,PageError500, ApplyContentStaffOtherFactory } from '../app/Model';\nimport { UploadFile, UploadProps } from 'antd/lib/upload/interface';\nimport { HttpResponse, HttpRequest } from '../app/Request';\nimport { withRouter } from 'react-router';\nimport ApplyUtils from '../utils/ApplyUtils';\nimport ImageViewer from './ImageViewer';\n\ntype StaffOtherProps=AppComponentProps&{\n    /**当前的申请单号 */\n    serialNumber:string;\n    /**是否禁止输入 */\n    disabled:boolean;\n    /**内容修改时处理 */\n    onChange(key:string,staffs:Array<any>):void;\n}\n\n/**\n * 人员管理组件：其他人员\n */\nclass StaffOther extends React.Component<StaffOtherProps>{\n    state:{\n        /**编辑抽屉是否显示 */\n        drawerVisible:boolean;\n        /**当前列表数据 */\n        data?:Array<ApplyContentStaffOther>;\n        /**正在编辑的数据 */\n        editingData?:ApplyContentStaffOther;\n        /**正在编辑的数据在列表中的索引 */\n        currentIndex:number;\n        /**文件上传验证错误 */\n        hasFileValidateError:boolean;\n        /**上传文件列表 */\n        fileList:{\n            [key:string]:Array<UploadFile>\n        };\n        /**上传是否成功 */\n        uploadSuccess:{\n            [key:string]:boolean\n        };\n        /**上传的数量限制 */\n        uploadLimit:{\n            [key:string]:number\n        }\n    }= { \n        drawerVisible: false,\n        currentIndex:-1,\n        hasFileValidateError:false,\n        fileList:{\n            \"identityFront\":[],\n            \"identityBack\":[],\n            \"qualification\":[],\n        },\n        uploadSuccess:{\n            \"identityFront\":true,\n            \"identityBack\":true,\n            \"qualification\":true,\n        },\n        uploadLimit:{\n            \"identityFront\":1,\n            \"identityBack\":1,\n            \"qualification\":20,\n        },\n    };\n\n    private key=\"applyTechnicians\";\n\n    /**\n     * 上传组件配置：图片\n     */\n    private readonly uploadPhotoConfig:UploadProps={\n        accept:\".jpg,.png\",\n        listType:\"picture-card\",\n        className:\"avatar-uploader\",\n        showUploadList:true,\n    }\n\n    /**\n     * 页面布局\n     */\n    render(){\n        if(!this.state.data){\n            return (<Row>\n                <Col span={24}>\n                    <Spin className=\"card-loading\" size=\"large\" tip=\"载入中...\" />\n                </Col>\n            </Row>);\n        }\n\n        const {getFieldDecorator} = this.props.form;\n        const {Meta}=Card;\n        const {Option}=Select;\n\n        const {data,editingData}=this.state;\n        const education=DictionaryLoader.instance.education();\n        const job=DictionaryLoader.instance.job(ApplyJobAndTitleTag.OTHER.tags);\n        const jobLabel=DictionaryLoader.instance.jobLabel;\n\n        return (\n            <div>\n                <Row>\n                    {data.map((person:ApplyContentStaffOther,index)=>(\n                            <Col span={5} offset={index%4!==0?1:0}>\n                                <Card bodyStyle={{paddingBottom:8}}\n                                    actions={[\n                                        <Button type=\"link\" onClick={()=>this.showDrawer(index)} disabled={this.props.disabled}>\n                                            <Icon type=\"edit\" key=\"edit\"/>\n                                        </Button>,\n                                        <Button type=\"link\" onClick={()=>this.deleteStaff(index)} disabled={this.props.disabled}>\n                                            <Icon type=\"delete\" key=\"delete\" />\n                                        </Button>\n                                    ]}\n                                >\n                                    <Meta className=\"staff-meta\"\n                                        title={person.name}\n                                        description={\n                                            <p>\n                                                {jobLabel(person.position)}<br/>\n                                                {jobLabel(person.title)}<br/>\n                                            </p>\n                                        }\n                                    />\n                                </Card>\n                            </Col>\n                        ))}\n                    <Col span={5} offset={1}>\n                        <Button type=\"dashed\" \n                            style={{width:\"100%\",height:162}}\n                            onClick={()=>this.showDrawer(-1)}\n                            disabled={this.props.disabled}\n                        >\n                            <Icon type=\"plus\" /><br/>\n                            添加\n                        </Button>\n                    </Col>\n                </Row>\n                \n                {/* 编辑抽屉 */}\n                {editingData?(\n                    <Drawer\n                        title={\n                            this.state.currentIndex===-1\n                                ?\"添加其他人员\"\n                                :`编辑人员（${editingData.name}）`\n                        } \n                        width={560}\n                        bodyStyle={{paddingBottom:54}}\n                        onClose={this.onClose}\n                        visible={this.state.drawerVisible}\n                        destroyOnClose={true}\n                    >\n                        <Form labelCol={{span:6}} wrapperCol={{span:18}}>\n                            <h4 className=\"title-form-sub\">信息填写</h4>\n                            <Row>\n                                <Col span={15}>\n                                    <Form.Item label=\"人员姓名\">{\n                                        getFieldDecorator(\"name\",{\n                                            initialValue:editingData.name,\n                                            rules:[\n                                                {required:true,message:\"人员姓名不能为空！\"}\n                                            ]\n                                        })(\n                                            <Input placeholder=\"请输入\" \n                                            />\n                                        )}\n                                    </Form.Item>\n                                </Col>\n                                <Col span={9}>\n                                    <Form.Item label=\"性别\" \n                                        labelCol={{span:8}} wrapperCol={{span:16}}\n                                    >{\n                                        getFieldDecorator(\"sex\",{\n                                            initialValue:(\"\"===editingData.sex?\"男\":editingData.sex),\n                                            rules:[\n                                                {required:false}\n                                            ]\n                                        })(\n                                            <Radio.Group\n                                            >\n                                                <Radio value=\"男\">男</Radio>\n                                                <Radio value=\"女\">女</Radio>\n                                            </Radio.Group>\n                                        )}\n                                    </Form.Item>\n                                </Col>\n                            </Row>\n                            <Row>\n                                <Col span={15}>\n                                    <Form.Item label=\"身份证号\">{\n                                        getFieldDecorator(\"idcard\",{\n                                            initialValue:editingData.idcard,\n                                            rules:[\n                                                {required:true,message:\"身份证号码不能为空！\"}\n                                            ]\n                                        })(\n                                            <Input placeholder=\"请输入\" \n                                            />\n                                        )}\n                                    </Form.Item>\n                                </Col>\n                                <Col span={9}>&nbsp;</Col>\n                            </Row>\n                            <Row>\n                                <Col span={15}>\n                                    <Form.Item label=\"毕业院校\">{\n                                        getFieldDecorator(\"school\",{\n                                            initialValue:editingData.school,\n                                            rules:[\n                                                {required:false}\n                                            ]\n                                        })(\n                                            <Input placeholder=\"请输入\" \n                                            />\n                                        )}\n                                    </Form.Item>\n                                </Col>\n                                <Col span={9}>\n                                    <Form.Item label=\"学历\" \n                                        labelCol={{span:8}} wrapperCol={{span:16}}\n                                    >{\n                                        getFieldDecorator(\"education\",{\n                                            initialValue:(\"\"===editingData.education?undefined:editingData.education),\n                                            rules:[\n                                                {required:false}\n                                            ]\n                                        })(\n                                            <Select showSearch\n                                                placeholder=\"请选择\" \n                                                optionFilterProp=\"children\" \n                                                filterOption={(input, option:React.ReactElement) =>\n                                                    option.props.children.toLowerCase().indexOf(input.toLowerCase()) >= 0\n                                                }\n                                            >\n                                                {education.map((data:DictionaryData,index:number)=>{\n                                                    return (<Option key={data.value}>{data.label}</Option>);\n                                                })}\n                                            </Select>\n                                        )}\n                                    </Form.Item>\n                                </Col>\n                            </Row>\n                            <Row>\n                                <Col span={15}>\n                                    <Form.Item label=\"职务职称\">{\n                                        getFieldDecorator(\"positionAndTitle\",{\n                                            initialValue:(\"\"===editingData.position&&\"\"===editingData.title?undefined:[editingData.position,editingData.title]),\n                                            rules:[\n                                                {required:true,message:\"职务职称不能为空！\"}\n                                            ]\n                                        })(\n                                            <Cascader \n                                                options={job} placeholder=\"请选择\" \n                                            />\n                                        )}\n                                    </Form.Item>\n                                </Col>\n                                <Col span={9}>&nbsp;</Col>\n                            </Row>\n                            <h4 className=\"title-form-sub\">上传内容</h4>\n                            <Row>\n                                <Col span={12}>\n                                    {this.uploadJsxContent(\"identityFront\",\"身份证正面\",true,\"示例图\")}\n                                </Col>\n                                <Col span={12}>\n                                    {this.uploadJsxContent(\"identityBack\",\"身份证背面\",true,\"示例图\")}\n                                </Col>\n                            </Row>\n                            <Row>\n                                <Col span={24}>\n                                    {this.uploadJsxContent(\"qualification\",\"资格证书\",false)}\n                                </Col>\n                            </Row>\n                        </Form>\n                        <div className=\"bottom-bar\">\n                            <Button onClick={this.onClose} style={{ marginRight: 8 }}>\n                                取消\n                            </Button>\n                            <Button onClick={this.saveStaff} type=\"primary\">\n                                保存\n                            </Button>\n                        </div>\n                    </Drawer>\n                ):null}\n            </div>\n        );\n    }\n\n    /**\n     * 组件载入后读取人员列表\n     */\n    componentDidMount=async ()=>{\n        // 获取其他人员信息\n        const staffResponse:HttpResponse<Array<ApplyContentStaffOther>>=\n            await ((new HttpRequest()).get<Array<ApplyContentStaffOther>>(\n                `webapi/apply/otherPerson?applyNo=${this.props.serialNumber}`\n            ));\n        // 失败跳转500\n        if(!staffResponse.success||!staffResponse.data){\n            this.props.history.push(PageError500);\n            return;\n        }\n\n        // 设置显示\n        this.setState({\n            ...this.state,\n            data:staffResponse.data\n        });\n        this.props.onChange(this.key,staffResponse.data);\n    }\n\n    /**\n     * 返回指定的图片上传单元\n     */\n    uploadJsxContent=(key:string,label:string,mustUpload:boolean,example?:string)=>(\n        <Form.Item\n            label={label} className={`staffother-${key}-photos ${(mustUpload?\"must-upload\":\"\")}`}\n            labelCol={{span:24}} wrapperCol={{span:24}}\n            validateStatus={\n                !this.state.hasFileValidateError\n                    ?(this.state.uploadSuccess[key]?\"\":\"error\")\n                    :(this.state.fileList[key].length>0?\"\":\"error\")\n            }\n            help={\n                !this.state.hasFileValidateError\n                    ?(this.state.uploadSuccess[key]?\"支持扩展名：.jpg .png\":\"文件上传失败！\")\n                    :(this.state.fileList[key].length>0?\"支持扩展名：.jpg .png\":\"必须上传此文件！\")\n            }\n        >\n            <Row>\n                <Col span={10}>\n                    <Upload {...this.uploadPhotoConfig}\n                        name={key}\n                        customRequest={(fileObject:object)=>{\n                            this.uploadPhoto(fileObject,key);\n                        }}\n                        onRemove={(file:UploadFile)=>{\n                            this.uploadPhotoRemove(file,key)\n                        }}\n                        onPreview={(file:UploadFile)=>{\n                            this.uploadPhotoPreview(file,key)\n                        }}\n                        fileList={this.state.fileList[key]}\n                        disabled={this.props.disabled}\n                    >\n                        {this.state.uploadLimit[key]>this.state.fileList[key].length\n                        ?(\n                            <div>\n                                <Icon type=\"plus\" />\n                                <div className=\"ant-upload-text\">上传</div>\n                            </div>\n                        )\n                        :null}\n                    </Upload>\n                </Col>\n                {example?(\n                    <Col span={8} offset={1}>\n                        <Button type=\"dashed\" style={{height:102,width:102}}>\n                            <Icon type=\"file-image\" style={{fontSize:32}} />\n                            <br/>\n                            <span>{example}</span><Icon type=\"zoom-in\" />\n                        </Button>\n                    </Col>\n                ):null}\n            </Row>  \n        </Form.Item>\n    );\n\n    /**\n     * 打开抽屉\n     */\n    showDrawer = (index:number) => {\n        // 获取数据\n        const data=(index===-1||!this.state.data\n            ?ApplyContentStaffOtherFactory.create(this.props.serialNumber)\n            :this.state.data[index]);\n\n        // 转换图片列表\n        let {identityFront,identityBack,qualification}=this.state.fileList;\n        const identityArray=data.idcardImg.split(\",\");\n        const idFrontString=identityArray[0],\n            idBackString=identityArray.length>1?identityArray[1]:\"\";\n        identityFront.splice(0,identityFront.length);\n        identityFront=ApplyUtils.urlStringToUploadArray(identityFront,idFrontString);\n        identityBack.splice(0,identityBack.length);\n        identityBack=ApplyUtils.urlStringToUploadArray(identityBack,idBackString);\n        qualification.splice(0,qualification.length);\n        qualification=ApplyUtils.urlStringToUploadArray(qualification,data.qualificationImg);\n\n        // 设置显示\n        this.setState({\n            ...this.state,\n            drawerVisible: true,\n            editingData:data,\n            currentIndex:index,\n            fileList:{\n                identityFront:identityFront,\n                identityBack:identityBack,\n                qualification:qualification,\n            }\n        });\n    };\n\n    /**\n     * 关闭抽屉\n     */\n    onClose = () => {\n        this.setState({\n            ...this.state,\n            drawerVisible: false,\n        });\n    };\n\n    /**\n     * 上传图片\n     */\n    uploadPhoto=async (fileObject:object,key:string)=>{\n        // 上传文件或图片\n        const option=fileObject as {file:File};\n        const uploadResponse=await (new HttpRequest())\n            .upload<{url:string},{applyNo:string}>\n            (\n                `webapi/comm/uploadFile`,\n                option.file,\n                \"file\",\n                {applyNo:this.props.serialNumber}\n            );\n        const {uploadSuccess}=this.state;\n        uploadSuccess[key]=(uploadResponse.success&&(uploadResponse.data!==null));\n        \n        //上传失败显示\n        if(!uploadSuccess[key]||!uploadResponse.data){\n            this.setState({\n                ...this.state,\n                uploadSuccess:uploadSuccess\n            });\n            return;\n        }\n\n        //上传成功，设置已上传列表\n        const {fileList}=this.state;\n        fileList[key].push({\n            uid:String(fileList[key].length),\n            name:option.file.name,\n            size:0,\n            type:\"\",\n            url:uploadResponse.data.url\n        });\n        this.setState({\n            ...this.state,\n            uploadSuccess:uploadSuccess,\n            fileList:fileList\n        });\n    }\n\n    /**\n     * 全屏查看上传的图片\n     */\n    uploadPhotoPreview=(uploadFile:UploadFile,key:string)=>{\n        // 获取全部上传的图片元素\n        const row=document.querySelector<HTMLDivElement>(`.staffother-${key}-photos`);\n        if(!row) return;\n        const imgArray=row.querySelectorAll<HTMLImageElement>\n            (\".ant-upload-list-item-image\");\n        const imageList:Array<HTMLImageElement>=[];\n        const {fileList}=this.state;\n\n        const viewImageNameSet=ApplyUtils.uploadArrayToNameSet(fileList[key]);\n        let currentIndex=0;\n        imgArray.forEach((img,index)=>{\n            if(viewImageNameSet.has(img.alt)){\n                imageList.push(img);\n                currentIndex=(img.alt===uploadFile.name?index:currentIndex);\n            }\n        });\n\n        ImageViewer.view(imageList,currentIndex);\n    }\n\n    /**\n     * 删除上传的文件或图片\n     */\n    uploadPhotoRemove=(file:UploadFile,key:string)=>{\n        const {confirm}=Modal;\n        const {fileList}=this.state;\n        confirm({\n            title: '确定要删除当前上传的图片么？',\n            okText: '继续删除',\n            okType: 'danger',\n            cancelText: '取消',\n            onOk:()=>{\n                fileList[key].splice(fileList[key].findIndex(item=>item.uid===file.uid),1);\n                this.setState({\n                    ...this.state,\n                    fileList:fileList\n                });\n            },\n        });\n    }\n\n    /**\n     * 保存添加的人员\n     */\n    saveStaff=()=>{\n        //校验表单\n        this.props.form.validateFieldsAndScroll((error,values)=>{\n            if(error||!this.state.data) return;\n            \n            //格式转换\n            values.birthday=!values.birthday||values.birthday.format(\"YYYY-MM-DD\");\n            values.position=values.positionAndTitle[0];\n            values.title=values.positionAndTitle[1];\n            delete values.positionAndTitle;\n\n            // 验证文件列表\n            const {identityFront,identityBack,qualification}=this.state.fileList;\n            if(identityFront.length<1\n                ||identityBack.length<1\n            ){\n                this.setState({\n                    ...this.state,\n                    hasFileValidateError:true\n                });\n                return;\n            }\n\n            // 转换文件列表\n            const identityImages=ApplyUtils.uploadArrayToUrlString(\n                identityFront.concat(identityBack)\n            );\n            const qualificationImages=ApplyUtils.uploadArrayToUrlString(qualification);\n\n            // 写入队列中\n            const {data,currentIndex}=this.state;\n            // 添加\n            if(currentIndex===-1){\n                data.push({\n                    ...values,\n                    idcardImg:identityImages,\n                    qualificationCertImg:qualificationImages,\n                });\n            }\n            // 编辑\n            else{\n                const currentData=data[currentIndex];\n                data[currentIndex]={\n                    ...currentData,\n                    ...values,\n                    idcardImg:identityImages,\n                    qualificationCertImg:qualificationImages,\n                }\n            }\n            this.setState({\n                ...this.state,\n                drawerVisible:false,\n                data:data\n            });\n            this.props.onChange(this.key,data);\n        });\n    }\n\n    /**\n     * 删除人员\n     */\n    deleteStaff=(index:number)=>{\n        if(!this.state.data) return;\n        // 获取人员信息\n        const {data}=this.state;\n        const staff=data[index];\n\n        const {confirm}=Modal;\n        confirm({\n            title: `确定要删除人员【${staff.name}】么？`,\n            okText: '继续删除',\n            okType: 'danger',\n            cancelText: '取消',\n            onOk:()=>{\n                data.splice(index,1);\n                this.setState({\n                    ...this.state,\n                    data:data\n                });\n                this.props.onChange(this.key,data);\n            },\n        });\n    }\n}\nexport default withRouter(Form.create<StaffOtherProps>({name:\"otherForm\"})(StaffOther));"]},"metadata":{},"sourceType":"module"}